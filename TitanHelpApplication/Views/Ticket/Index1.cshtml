@model IEnumerable<TitanHelpApplication.Models.Ticket>

@{
    ViewData["Title"] = "Ticket List";
}

<h1>Support Tickets</h1>

<table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>Description</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Title</td>
                <td>@item.Description</td>
                <td>@item.Status</td>
            </tr>
        }
    </tbody>
</table>